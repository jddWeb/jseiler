{
    "name": "jensseiler/jssitepackage",
    "type": "typo3-cms-extension",
    "description": "Sitepackage Jens Seiler",
    "require": {
        "typo3/cms-core": "^13.4",
        "friendsoftypo3/content-blocks": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "JensSeiler\\JsSitepackage\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "jssitepackage",
            "site-sets": {
                "jssitepackage": "Configuration/Sets/jssitepackage"
            },
            "content-blocks": [
                "EXT:jssitepackage/ContentBlocks/"
            ]
        }
    }
}
